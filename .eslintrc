{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": [2, 120, 2],
    "no-multi-spaces": 0,
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true,
      "align": "value"
    }],
    "comma-dangle": [2, "never"],
    "template-curly-spacing": [2, "always"],
    "arrow-body-style": 0,
    "react/sort-comp": 0,
    "react/jsx-equals-spacing": 0,
    "react/jsx-curly-spacing": 0,
    "import/no-unresolved": 0
  },
  "plugins": [
    "babel",
    "react"
  ]
}
